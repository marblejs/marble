  "bugs": {
    "url": "https://github.com/marblejs/marble/issues"
  },
  "homepage": "https://github.com/marblejs/marble#readme",
  "dependencies": {
    "@marblejs/core": "^4.0.1",
    "@marblejs/http": "^4.0.1",
    "@marblejs/middleware-body": "^4.0.1",
    "@marblejs/middleware-cors": "^4.0.1",
    "@marblejs/middleware-io": "^4.0.1",
    "@marblejs/middleware-logger": "^4.0.1",
    "@marblejs/middleware-multipart": "^4.0.1",
    "@marblejs/testing": "^4.0.1",
    "@marblejs/websockets": "^4.0.1",
    "@types/node": "^10.1.2",
    "fp-ts": "^2.8.2",
